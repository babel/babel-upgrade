    "cross-spawn": "^6.0.4",
    "globby": "^8.0.1",
    "has-yarn": "^1.0.0",
    "json5": "^0.5.1",
    "pify": "^3.0.0",
    "read-pkg-up": "^3.0.0",
    "rimraf": "^2.6.2",
    "semver": "^5.5.0",
    "sort-keys": "^2.0.0",
    "write": "^1.0.3",
    "write-json-file": "^2.3.0",
    "write-pkg": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.40",
